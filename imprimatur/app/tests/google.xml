<?xml version="1.0"?>
<!DOCTYPE imprimatur PUBLIC 
    "-//Imprimatur DTD 007//EN"
    "http://imprimatur.sourceforge.net/imprimatur-007.dtd">
<imprimatur port="80" hostname="www.google.com">
	<test name="Check Google homepage">
		<request path="/">

			<!-- Check the HTTP response code is OK	-->
			<response-code value="200" />

			<!-- Regular expression	-->
			<regex pattern=".*Google.*" />
		</request>
	</test>
</imprimatur>

