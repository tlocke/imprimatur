<?xml version="1.0"?>
<!DOCTYPE imprimatur PUBLIC 
    "-//Imprimatur DTD 002//EN"
    "http://imprimatur.sourceforge.net/imprimatur-002.dtd">
<imprimatur port="80" hostname="www.google.com">
  <test name="Test Google Directory">
    <request path="/dirhp?hl=en" method="get">
      <response-code value="200" />

      <regex pattern=".*Food.*"/>

      <regex pattern=".*Arts.*"/>
    </request>

    <request path="/Top/Recreation/Food/Cheese/" method="get">
      <response-code value="200" />

      <regex pattern=".*Cheese Making.*"/>
    </request>
  </test>

  <test name="Test Alerts">
    <request path="/alerts" method="get">
      <response-code value="200" />

      <regex pattern=".*privacy policy.*"/>
    </request>
  </test>
</imprimatur>

