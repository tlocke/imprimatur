<?xml version="1.0"?>
<!DOCTYPE imprimatur PUBLIC 
    "-//Imprimatur DTD 002//EN"
    "http://imprimatur.sourceforge.net/imprimatur-002.dtd">
<imprimatur port="8080" hostname="localhost">
  <test name="File upload">
    <request path="/importData.jsp" method="post"
    enctype="multipart/form-data" wait-for-refreshes="true">
      <parameter name="id" value="1" />

      <parameter type="file" name="importFile"
      value="workspace/theelected/test/meps.csv" />

      <regex pattern=".*Uploaded ok..*"/>

      <response-code value="200" />
    </request>
  </test>
</imprimatur>

