<?xml version="1.0"?>
<!DOCTYPE imprimatur PUBLIC 
    "-//Imprimatur DTD 001//EN"
    "http://imprimatur.sourceforge.net/imprimatur-001.dtd">
<imprimatur port="8080" hostname="localhost">
	<test name="institution-import-invalid">

		<request path="/editor/login" method="post">
			<parameter name="emailAddress"
				value="root@localhost.localdomain" />

			<parameter name="password" value="bridle" />

			<response-code value="200" />

			<regex pattern=".*The areas that you are responsible for are.*"/>
		</request>

		<request path="/editor/logout" method="post">
			<response-code value="200" />
		</request>
	</test>
</imprimatur>

